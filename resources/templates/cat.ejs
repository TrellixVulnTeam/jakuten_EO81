<div class="row">
    <div class="col-lg-12">
        <h1><%- category %></h1>
    </div>
</div>
<% items.forEach(function(item){ %>
<div class="row mb-3">
    <div class="col-lg-8">
        <a href="/item/<%- item.id %>">
            <img src="/img/items/<%- item.sku %>_m.jpg" class="img-fluid rounded"/>
        </a>
    </div>
    <div class="col-lg-4">
        <h2><%- item.name %></h2>
        <p><%- item.title %></p>
    </div>
</div>
<% }) %>
    