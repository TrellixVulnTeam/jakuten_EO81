<div class="row">
    <div class="col-lg-12">
        <h1>Pickups</h1>
    </div>
</div>
<div class="row mb-2">
    <% cards.forEach(function(card){ %>
    <div class="col-lg-4">
        <div class="card m-2 h-auto">
            <a href="/item/<%- card.id %>">
                <img class="card-img-top" src="/img/items/<%- card.sku %>_s.jpg"/>
            </a>
            <div class="card-body h-100">
                <h5 class="card-title"><%- card.name %></h5>
                <p class="card-text"><%- card.title %></p>
            </div>
        </div>
    </div>
    <% }) %>
</div>
